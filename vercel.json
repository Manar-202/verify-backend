{
    "version": 2,
    "functions": {
        "api/*.js": {
        "maxDuration": 300
        }
    },
    
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ]
  }
  